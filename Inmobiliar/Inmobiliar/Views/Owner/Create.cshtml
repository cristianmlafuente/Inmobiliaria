@model Inmobiliar.Models.PersonasModel
@{
    ViewBag.Title = "Clientes";
}

<div id="page-wrapper">
    <div id="page-inner">
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h1 class="page-header">
                            <small>Nuevo Cliente</small>
                        </h1>
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            @*<div class="col-lg-6">*@
                                <form role="form" action="Create" method="post">
                                    <div class="form-group has-success col-lg-6">
                                        <label class="control-label" for="inputSuccess">Nombre</label>
                                        <input type="text" class="form-control" id="" name="Nombre">
                                    </div>                                    
                                    <div class="form-group has-success col-lg-6">
                                        <label class="control-label" for="inputSuccess">DU</label>
                                        <input type="text" class="form-control" id="" name="DU">
                                    </div>                                    
                                    <div class="form-group has-success col-lg-6">
                                        <label class="control-label" for="inputSuccess">Telefono</label>
                                        <input type="text" class="form-control" id="" name="Telefono">
                                    </div>                                    
                                    <div class="form-group has-success col-lg-6">
                                        <label class="control-label" for="inputSuccess">TelefonoLaboral</label>
                                        <input type="text" class="form-control" id="" name="TelefonoLaboral">
                                    </div>     
                                    <div class="form-group has-success col-lg-6">
                                        <label class="control-label" for="inputSuccess">Apellido</label>
                                        <input type="text" class="form-control" id="" name="Apellido">
                                    </div>
                                    <div class="form-group has-success col-lg-6">
                                        <label class="control-label" for="inputSuccess">Email</label>
                                        <input type="text" class="form-control" id="" name="Email">

                                    </div>
                                    <div class="form-group has-success col-lg-6">
                                        <label class="control-label" for="inputSuccess">Celular</label>
                                        <input type="text" class="form-control" id="" name="Celular">
                                    </div>                               
                                    <div class="controls">
                                        <input type="submit" class="btn btn-primary" value="Enviar" />
                                    </div>
                                </form>
                            @*</div>*@
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>