@model Inmobiliar.Models.CobroAlquilerModel

    <div id="page-wrapper">
        <div id="page-inner">
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h3>Cobro Alquileres
                            </h3>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            Buscar Inquilino
                                        </div>
                                        <div class="panel-body">
                                            <form role="form" action="Create" method="post">
                                                <div class="form-group has-success">
                                                    <label class="control-label" for="inputSuccess">Inquilino</label>
                                                    <input type="text" class="form-control" id="ownerInquilino" name="Inquilino">
                                                </div>
                                                <div class="form-group has-success">
                                                    <label class="control-label" for="inputSuccess">Apellido</label>
                                                    <input type="text" class="form-control" id="ownerApellido" name="Apellido" disabled="" placeholder="@Model.Contrato.Inquilino.Apellido">
                                                </div>
                                                <div class="form-group has-success">
                                                    <label class="control-label" for="inputSuccess">Nombre</label>
                                                    <input type="text" class="form-control" id="ownerName" name="Nombre" disabled="" placeholder="@Model.Contrato.Inquilino.Nombre">
                                                </div>
                                                <div class="form-group has-success">
                                                    <label class="control-label" for="inputSuccess">DU</label>
                                                    <input type="text" class="form-control" id="" name="DU" disabled="" placeholder="@Model.Contrato.Inquilino.DU">
                                                </div>
                                                <div class="form-group has-success">
                                                    <label class="control-label" for="inputSuccess">TelefonoLaboral</label>
                                                    <input type="text" class="form-control" id="" name="TelefonoLaboral" disabled="" placeholder="@Model.Contrato.Inquilino.TelefonoLaboral">
                                                </div>
                                            </form>
                                        </div>
                                    </div>                                    
                                </div>
                                <div class="col-lg-6" id="Propiedad">
                                    @{Html.RenderPartial("../Propiedades/_PropiedadResumida", Model.Contrato.Propiedades);}
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12" id="Cobro">
                                    <div class="panel panel-warning">
                                        <div class="panel-heading">
                                            Detalles del Cobro
                                        </div>
                                        <div class="panel-body">
                                            <div class="col-md-6 col-sm-6">
                                                <div class="well">
                                                    <form role="form" action="Create" method="post">
                                                        <div class="form-group">
                                                            <label class="control-label" for="disabledSelect">Periodo</label>
                                                            <select class="form-control" id="Periodos" >
                                                                <option disabled selected>Seleccione</option>
                                                                @foreach (var item in Model.Periodos)
                                                                {
                                                                    <option value="@item.MesAño">@item.Detalle</option>
                                                                }
                                                            </select>                                                             
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                            <div class="col-md-6 col-sm-6">
                                                @{Html.RenderPartial("_Observaciones");}                                      
                                            </div>                                            
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
