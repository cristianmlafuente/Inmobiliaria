@model Inmobiliar.Models.PropiedadesModel
@*@{
    ViewBag.Title = "Create";
}*@

<div id="page-wrapper">
    <div id="page-inner">
        <h2>Create</h2>

        <form role="form" action="Create" method="post" >
            <div class="form-group has-success">
                <label class="control-label" for="inputSuccess">Propiedad</label>
                <input type="text" class="form-control" id="" name="Direccion">
                
            </div>
            <div class="form-group has-warning">
                <label class="control-label" for="inputWarning">Tipo</label>
                <input type="text" class="form-control" id="" name="Tipo">
            </div>
            <div class="form-group has-error">
                <label class="control-label" for="inputError">Dueño</label>
                <input type="text" class="form-control" id="" name="Dueño">
            </div>
            <div class="controls">
                <input type="submit" class="btn btn-primary" value="Enviar" />
            </div>
        </form>
    </div>
</div>
